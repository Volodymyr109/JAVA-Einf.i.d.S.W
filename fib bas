public class Main {
    public static void main(String[] args) {

        int f1 = 1;
        int f2 = 1;
        int f3; //sum f1 und f2
        System.out.println("| " + f1 + " |  f(n): " + f2 +  "                          |");
        System.out.print("+---+-----------------------------------+" + "\n");

        for (int i = 2; i <= 10; i++) {

            f3 = f1 + f2;
            System.out.print("| " + f3 + " ");
            f1 = f2;
            f2 = f3;

        }
    }
}
